{
  "name": "lottie-lint",
  "version": "1.1.6",
  "description": "lottie json lint",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -fr ./lib && babel src -d lib",
    "lint": "eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha --require intelli-espower-loader --require @babel/register --check-leaks -t 5000 test/*.test.js",
    "cov": "nyc --reporter=clover --reporter=text-summary npm run test",
    "cov:html": "nyc --reporter=html --reporter=text-summary npm run test",
    "travis": "npm run cov"
  },
  "keywords": [
    "lottie",
    "lint",
    "performance"
  ],
  "files": [
    "lib",
    "index.d.ts"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "author": "lottie lint team",
  "license": "MIT",
  "dependencies": {
    "pica": "^5.3.0",
    "semver": "^5.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "eslint": "^7.5.0",
    "eslint-config-egg": "7.3.0",
    "eslint-plugin-jsdoc": "^36.0.6",
    "eslint-plugin-node": "^11.1.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^6.2.2",
    "nyc": "^15.1.0",
    "power-assert": "^1.6.1"
  }
}
